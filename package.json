{
    "name": "montage",
    "description": "A UI Framework",
    "version": "0.13.0-1",
    "publishConfig": {
        "tag": "bleeding-edge"
    },
    "main": "montage",
    "bin": {
        "montage": "bin/montage"
    },
    "overlay": {
        "browser": {
            "main": "core/core",
            "mappings": {
                "mr": {
                    "name": "mr",
                    "location": "packages/mr"
                },
                "q": {
                    "name": "q",
                    "location": "packages/mr/packages/q"
                }
            },
            "exclude": [
                "lab",
                "etc",
                "node.js"
            ]
        }
    },
    "dependencies": {
        "collections": "~0.1.14",
        "frb": "~0.1.16"
    },
    "devDependencies": {
        "mr": "0.12 - 0.13",
        "q": "0.8 - 0.9",
        "q-io": "1.3 - 2",
        "jsdom": "0.2 - 0.3"
    },
    "bin": {
        "montage": "bin/montage"
    }
}
